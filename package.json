{
  "name": "givemetime",
  "version": "0.0.1",
  "description": "Project to give some time to a R&D project suggested by a team member",
  "main": "src/index.jsx",
  "pre-commit": "test",
  "scripts": {
    "test": "npm run --loglevel=silent test:lint && npm run --loglevel=silent test:unit",
    "db": "./scripts/db.sh up",
    "db:rm": "./scripts/db.sh rm",
    "db:migration:create": "./scripts/db-migrate.sh --sql-file create",
    "db:migrate": "./scripts/db-migrate.sh up",
    "db:revert": "./scripts/db-migrate.sh down",
    "db:test": "./scripts/db-test.sh",
    "start": "./node_modules/.bin/webpack-dev-server --config ./webpack.config.js -d --inline --hot --host localhost --open",
    "webpack": "./node_modules/.bin/webpack --config webpack.config.js",
    "server": "./scripts/server.sh",
    "psql": "./scripts/psql.sh",
    "build": "./node_modules/.bin/webpack ./webpack.config.js",
    "test:lint": "./node_modules/.bin/eslint . --ext .js --ext .jsx",
    "test:lint:fix": "npm run --loglevel=silent test:lint -- --fix",
    "test:unit": "./node_modules/.bin/mocha --compilers js:babel-register --recursive $(find src -name '*.spec.js')",
    "test:unit:watch": "npm run --loglevel=silent test:unit -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inovia-team/givemetime.git"
  },
  "author": "inovia-team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/inovia-team/givemetime/issues"
  },
  "homepage": "https://github.com/inovia-team/givemetime#readme",
  "devDependencies": {
    "babel-cli": "~6.14.0",
    "babel-core": "~6.14.0",
    "babel-istanbul": "~0.11.0",
    "babel-loader": "~6.2.5",
    "babel-plugin-transform-object-rest-spread": "~6.8.0",
    "babel-preset-es2015": "~6.14.0",
    "babel-preset-react": "~6.11.1",
    "babel-preset-react-hmre": "~1.1.1",
    "css-loader": "^0.23.1",
    "db-migrate": "^0.9.25",
    "eslint": "^3.1.1",
    "eslint-plugin-react": "^5.1.1",
    "expect": "^1.20.1",
    "isparta": "^4.0.0",
    "isparta-instrumenter-loader": "^1.0.0",
    "istanbul": "^0.4.4",
    "mocha": "^3.0.0-1",
    "pre-commit": "^1.1.3",
    "react-addons-test-utils": "^15.1.0",
    "react-hot-loader": "^1.3.0",
    "redux-mock-store": "^1.1.0",
    "style-loader": "^0.13.1",
    "webpack": "~1.13.2",
    "webpack-dev-server": "~1.16.1",
    "webpack-merge": "~0.14.1",
    "eslint": "^3.1.1",
    "eslint-plugin-react": "^5.2.2",
    "isparta-instrumenter-loader": "^1.0.1",
    "fetch-mock": "^5.1.1"
  },
  "dependencies": {
    "material-ui": "^0.15.4",
    "mocha": "^3.0.0-1",
    "postgraphql": "^1.7.0",
    "prop-types": "^0.2.0",
    "react": "^15.3.1",
    "react-dom": "^15.1.0",
    "react-google-login": "^2.0.0",
